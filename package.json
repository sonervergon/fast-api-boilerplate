{
  "name": "typethon",
  "version": "1.0.0",
  "description": "A Typescript PNPM Monorepo containing Python API and related services",
  "private": true,
  "scripts": {
    "setup-env": "turbo setup-env",
    "dev": "turbo dev",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "format": "biome format --write .",
    "lint": "biome lint --write . && pnpm lint:sherif",
    "lint:sherif": "pnpm dlx sherif@latest",
    "lint:sherif:fix": "pnpm dlx sherif@latest --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "turbo": "^2.5.0"
  },
  "packageManager": "pnpm@10.8.0"
}
